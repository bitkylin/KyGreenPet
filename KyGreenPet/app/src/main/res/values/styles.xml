<resources>

  <!-- Base application theme. -->
  <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
    <!-- Customize your theme here. -->
    <item name="colorPrimary">@color/colorAccent</item>
    <item name="colorPrimaryDark">@color/colorAccent</item>
    <item name="colorAccent">@color/colorAccent</item>
  </style>


  <declare-styleable name="KyToolbar">
    <attr format="reference" name="KyNavigationIcon"/>
    <attr format="boolean" name="enableKyNavigationIcon"/>
    <attr format="reference" name="setRightButton"/>
    <attr format="string|reference" name="setRightText"/>
  </declare-styleable>
</resources>
